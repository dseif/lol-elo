include header
include footer
include matchlist

extends layout
block content
	div#screencontainer
		div#content
			+header()
			div#wrapper
				div#leftcontainer
					div#matchlist.contentlarge
						div.contentlargeheader
							h1 RESULTS
						h1
							if teamNames
								=teamNames[0]
							if eventId
								=eventId
						+matchlist(results)
				div#rightcontainer
					div#upcomingmatches
						div.contentlargeheader
							h1 MODULE 1
					div#upcomingmatches
						div.contentlargeheader
							h1 MODULE 2
					div#ad1(style='background-image:url("/img/v1.png")')
	include footer
